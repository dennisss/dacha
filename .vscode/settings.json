{
  "files.watcherExclude": {
    "**/target/**": true,
    "**/built/**": true,
    "**/built-rust/**": true,
    "**/built-config/**": true,
    "**/node_modules/*/**": true
  },
  "editor.scrollBeyondLastLine": false,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.checkOnSave.extraArgs": [
    "--target-dir",
    "/tmp/rust-analyzer-check"
  ],
  "files.associations": {
    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "complex": "cpp",
    "csetjmp": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "rope": "cpp",
    "slist": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "numeric": "cpp",
    "ratio": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "system_error": "cpp",
    "cfenv": "cpp",
    "cinttypes": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "typeinfo": "cpp",
    "cstddef": "cpp",
    "bit": "cpp",
    "map": "cpp",
    "algorithm": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "variant": "cpp",
    "module.h": "c",
    "bitset": "cpp",
